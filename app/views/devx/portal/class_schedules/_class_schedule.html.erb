<tr>
	<td> <%= class_schedule.name %> </td>
	<td> <%= class_schedule.description %> </td>
	<td> <%= class_schedule.start_time.try(:strftime, '%I:%M %P') %> </td>
	<td> <%= class_schedule.end_time.try(:strftime, '%I:%M %P') %> </td>
	<td>
		<%= link_to 'Edit', devx.edit_portal_classroom_class_schedule_path(@classroom, class_schedule) %> <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
		<%= link_to 'Delete', devx.portal_classroom_class_schedule_path(@classroom, class_schedule), method: :delete, data: { confirm: "Are you sure you want to delete this?" } %>
		<i class="fa fa-trash" aria-hidden="true"></i>
	</td>
</tr>
