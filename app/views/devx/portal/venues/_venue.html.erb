<tr>
    <td><%= link_to venue.name, devx.edit_portal_venue_path(venue) %></td>
    <td><%= venue.try(:full_address) %></td>
    <td>
    <%= link_to 'Edit', devx.edit_portal_venue_path(venue) %> <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
    <%= link_to 'Delete', devx.portal_venue_path(venue), method: :delete, data: { confirm: "Are you sure you want to delete #{venue.name}?" } %> <i class="fa fa-trash" aria-hidden="true"></i>
    </td>
</tr>