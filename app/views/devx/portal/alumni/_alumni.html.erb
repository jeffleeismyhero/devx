<tr>
  <td><%= link_to alumni.full_name, devx.edit_portal_alumni_path(alumni) %></td>
  <td><%= alumni.graduation_year %></td>
  <td><%= alumni.updated_at.try(:strftime, '%m/%d/%Y %I:%M') %></td>
  <td>
    <%= link_to 'Edit', devx.edit_portal_alumni_path(alumni) %>
    <%= link_to 'Delete', devx.portal_alumni_path(alumni), method: :delete, data: { confirm: "Are you sure you want to delete #{alumni.full_name}?" } %>
  </td>
</tr>