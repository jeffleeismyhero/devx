<% provide(:title, 'New Extracurricular') %>
<div class="container">
  <div class="row">
    <div class="col-12 columns">
      <h1><%= yield(:title) %></h1>
    </div>
    <div class="col-12">
      <%= link_to 'Back to Extracurriculars', devx.portal_extracurriculars_path %>

      <%= form_for @extracurricular, url: portal_extracurriculars_path, method: :post do |f| %>
        <% if @extracurricular.errors.any? %>
          <ul>
            <% @extracurricular.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
        <% end %>

        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>











