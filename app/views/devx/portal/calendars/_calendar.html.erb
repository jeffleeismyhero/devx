<tr>
    <td><%= link_to calendar.name, devx.portal_calendar_path(calendar) %></td>
    <td><%= calendar.active %></td>
    <td>
        <%= link_to 'Edit', devx.edit_portal_calendar_path(calendar) %> <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
        <%= link_to 'Delete', devx.portal_calendar_path(calendar), method: :delete, data: { confirm: "Are you sure you want to delete #{calendar.name}?" } %>  <i class="fa fa-trash" aria-hidden="true"></i>
    </td>
</tr>