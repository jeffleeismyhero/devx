<tr>
	<td> <%= classroom.name %> </td>
	<td> <%= classroom.classroom_teachers.count %> </td>
	<td> <%= classroom.active %> </td>
	<td> 
		<%= link_to 'Edit', devx.edit_portal_classroom_path(classroom) %> <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
		<%= link_to 'Delete', devx.portal_classroom_path(classroom), method: :delete, data: { confirm: "Are you sure you want to delete this?" } %> 
		<i class="fa fa-trash" aria-hidden="true"></i>
	</td>
</tr>