<% provide(:title, 'Edit Staff') %>

<div class="container">
  <div class="row">
    <div class="col-12 columns">
      <h1><%= yield(:title) %></h1>
    </div>
    <div class="col-12">
      <%= link_to 'Back to Staff', devx.portal_members_path %>
      <%= form_for @member, url: devx.portal_member_path(@member) do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>