<% provide(:title, 'Edit ' + controller_name.classify.pluralize) %>

<div class="container">
  <div class="row">
    <div class="large-12 columns">
      <h1><%= yield(:title) %></h1>
    </div>
    <div class="large-12">
      <%= link_to 'Back to members', devx.portal_members_path %>
      <%= form_for @member, url: devx.portal_member_path(@member) do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>