<% provide(:title, 'Create ' + controller_name.classify) %>

<div class="container">
  <div class="row">
    <div class="large-12 columns">
      <h1><%= yield(:title) %></h1>
    </div>
    <div class="large-12">
      <%= link_to 'Back to member', devx.portal_members_path %>
      <%= form_for @member, url: devx.portal_members_path, method: :post do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>