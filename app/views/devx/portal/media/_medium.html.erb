<tr>
    <td style="padding:10px;border-bottom:1px solid #ccc;"><%= medium.id %></td>
    <td style="padding:10px;border-bottom:1px solid #ccc;"><%= medium.name %></td>
    <td style="text-align:right; padding:10px;border-bottom:1px solid #ccc;"><%= link_to 'View', medium.file.url, target: '_blank' %> <i class="fa fa-picture-o" aria-hidden="true"></i>
      <%= link_to 'Delete', devx.portal_medium_path(medium), method: :delete, data: { confirm: "Are you sure you want to delete #{medium.name}?" } %>  <i class="fa fa-trash" aria-hidden="true"></i></td>
</tr>