<% provide(:title, 'Create ' + controller_name.classify.pluralize) %>

<div class="container">
  <div class="row">
    <div class="col-12 columns">
      <h1><%= yield(:title) %></h1>
    </div>
    <div class="col-12 columns">
      <%= link_to 'Back to Product SKUs', devx.portal_product_product_skus_path(@product) %>
      <%= form_for @product_sku, url: devx.portal_product_product_skus_path(@product), method: :post do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>