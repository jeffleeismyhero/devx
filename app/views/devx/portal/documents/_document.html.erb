<tr>
    <td><%= document.name %></td>
    <td><%= link_to 'View', document.file.try(:url) %></td>
    <td>
    <%= link_to 'Edit', devx.edit_portal_document_path(document) %> <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
    <%= link_to 'Delete', devx.portal_document_path(document), method: :delete, data: { confirm: "Are you sure you want to delete #{document.name}?" } %>  <i class="fa fa-trash" aria-hidden="true"></i>
    </td>
</tr>