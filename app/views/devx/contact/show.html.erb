<div class="container" style="margin-bottom:20px;">
    <div class="row">
        <div class="medium-10 medium-offset-1 large-10 large-offset-1 columns">
            <div class="small-12 medium-12 large-6 xlarge-6 columns">
                <form class="contact-form" style="margin:20px 0;">
                    <center>
                        <input type="text" name="firstname" placeholder="First Name*">
                        <input type="text" name="lastname" placeholder="Last Name*">
                        <input type="text" name="email" placeholder="E-mail Address*">
                        <input type="text" name="phone" placeholder="Phone Number">
                        <textarea name="message" placeholder="Message" style="width:99.5%; margin:5px 0; padding:15px;"></textarea>
                        <input style="cursor:pointer; margin:10px 0 0 0; padding:0; color:#384967;" type="submit" value="Submit">
                    </center>
                </form>
            </div>
            <% if app_branding.address.present? %>
                <div class="small-12 medium-12 large-6 xlarge-6 columns maps" style="margin:20px 0;">

                <%= "<iframe id=\"gmap_canvas\" src=\"https://www.google.com/maps/embed/v1/place?q=#{app_branding.address}&amp;key=AIzaSyAN0om9mFmy1QN6Wf54tXAowK4eT0ZUPrU\" width=\"100%\" height=\"410\" style=\"pointer-events: none;\"></iframe>".html_safe %>

                </div>
            <% end %>
        </div>
    </div>
</div>