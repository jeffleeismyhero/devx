<% @attributes[:events].try(:each) do |schedule| %>
<div class="medium-12 large-4 columns" style="margin-top:40px;">
  <div class="article-title"><%= schedule.event.name %></div>

  <div class="event-square small-3 medium-4 columns"><span class="event-month"><%= schedule.start_time.try(:strftime, '%b') %></span> <span class="event-date"><%= schedule.start_time.try(:strftime, '%d') %></span></div>

  <div class="small-9 medium-8 columns"><span><%= schedule.start_time.try(:strftime, '%I:%M%P') %></span><br>
    <span><%= schedule.event.location %></span><br>
    &nbsp;</div>
  </div>
<% end %>