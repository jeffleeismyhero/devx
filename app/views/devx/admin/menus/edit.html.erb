<div class="container">
  <div class="row">
    <div class="large-12">
      <h1>Edit <%= controller_name.classify %></h1>
    </div>
    <div class="large-12">
      <%= link_to 'Back to menus', devx.admin_menus_path %>
      <%= form_for @menu, url: devx.admin_menu_path(@menu) do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>