<div class="container">
  <div class="row">
    <div class="large-12 columns">
      <h1>Branding</h1>
    </div>
    <div class="large-12 columns">
      <%= form_for @branding, url: devx.admin_branding_path(@branding) do |f| %>
        <%= f.label :company_name %>
        <%= f.text_field :company_name %>
        <%= f.label :logo %>
        <%= f.file_field :logo %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>